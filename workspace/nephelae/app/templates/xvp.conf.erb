POOL <%= proxy.zone.name %>
  DOMAIN \"\"
  MANAGER root $(echo <%= proxy.encrypted_root_password %> | xvp -x)
<% proxy.zone.hosts.each do |host| %>
  HOST <%= host.ip %>
<% end %>
<% proxy.vnc_consoles.each do |console| %>
  <% if not console.virtual_machine.uninstalling? %>
    VM <%= console.port %> <%= console.virtual_machine.uuid %> $(echo <%= console.password %> | xvp -e)
  <% end %>
<% end %>
